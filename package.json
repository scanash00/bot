{
  "name": "aethel",
  "type": "module",
  "version": "1.10.0",
  "description": "A privacy-conscious, production-ready Discord user bot",
  "main": "src/index.js",
  "scripts": {
    "start": "node -r dotenv/config src/index.js",
    "dev": "nodemon -r dotenv/config src/index.js",
    "migrate": "node -r dotenv/config scripts/run-migrations.js",
    "lint": "eslint . --config eslint.config.cjs",
    "lint:fix": "eslint . --config eslint.config.cjs --fix",
    "lint:format": "eslint . --config eslint.config.cjs --format=codeframe",
    "format": "prettier --write \"**/*.{js,json,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,json,md}\" --ignore-path .prettierignore",
    "check": "pnpm run lint && pnpm run format:check",
    "compile:mo": "node scripts/compile-pos.js",
    "dev:ts": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "@discordjs/rest": "^2.5.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv": "^16.6.1",
    "eslint-plugin-prettier": "^5.5.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "gettext-parser": "^8.0.0",
    "helmet": "^8.1.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.6.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "po2json": "^0.4.5",
    "validator": "^13.15.15",
    "whois-json": "^2.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/gettext-parser": "^8.0.0",
    "@types/node": "^24.0.7",
    "@types/pg": "^8.15.4",
    "@types/validator": "^13.15.2",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
